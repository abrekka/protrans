<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping default-lazy="false">

<class 
    name="no.ugland.utransprod.model.TransportCostAddition" 
    table="TRANSPORT_COST_ADDITION"
>
<id name="transportCostAdditionId" type="java.lang.Integer" column="TRANSPORT_COST_ADDITION_ID">
        <generator class="identity"/>
        </id>

    

	<property
        name="description"
        type="java.lang.String"
        column="DESCRIPTION"
        length="100"
    />
	<property
        name="basis"
        type="java.lang.String"
        column="BASIS"
        length="100"
    />
    <property
        name="basis2"
        type="java.lang.String"
        column="BASIS2"
        length="100"
    />
	<property
        name="addition"
        type="java.math.BigDecimal"
        column="ADDITION"
        length="32"
    />
    
    <property
        name="metric"
        type="java.lang.String"
        column="METRIC"
        length="50"
    />
	<property
        name="transportBasis"
        type="java.lang.Integer"
        column="TRANSPORT_BASIS"
        length="32"
    />
	<property
        name="memberOfMaxAdditions"
        type="java.lang.Integer"
        column="MEMBER_OF_MAX_ADDITIONS"
        length="32"
    />
    

</class>
</hibernate-mapping>
